{
  "manifest_version": 3,
  "name": "AutoTrader Miles Overlay",
  "description": "Annotates AutoTrader listings with miles-for-year anomaly badges.",
  "version": "1.1.0",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.autotrader.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "anomalyModel.js"
      ],
      "matches": [
        "https://www.autotrader.com/*"
      ]
    }
  ]
}
